<tr>
  <th><%= question.id %></th>
  <th><%= question.body %></th>
  <th>
    <%= link_to 'Show', admin_question_path(question) %>
    |
    <%= link_to 'Edit', edit_admin_question_path(question) %>
    |
    <%= link_to 'Delete',
                admin_question_path(question),
                method: :delete, data: { confirm: 'Are you sure?' } %>
  </th>
</tr>
