<tr>
  <p>
    <th><%= test.id %></th>
    <th><%= test.title %></th>
    <th><%= test.questions.count %></th>
    <th>
        <%= button_to 'Start', start_test_path(test) %>
        <%= link_to 'Show', test_path(test) %>
        |
        <%= link_to 'Edit', edit_test_path(test) %>
        |
        <%= link_to 'Delete',
                  test_path(test),
                  method: :delete,
                  data: {confirm: 'Are you sure?'}%>
    </th>
  </p>
</tr>
